{
  "WhoAmI": "Development",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Repository": {
    "ThumbsBucket": "--supplied-by-env-var-from-tf-on-live--",
    "EnsureNewThumbnailLayout": true
  },  
  "ConnectionStrings": {
    "PostgreSQLConnection": "--supplied-by-env-var-from-tf-on-live--"
  },
  "AWS": {
    "Profile": "default",
    "Region": "eu-west-1"
  },
  "PathRules": {
    "Default": "/{prefix}/{customer}/{space}/{assetPath}",
    "Overrides": {
      "my-proxy.com": "/{prefix}/{assetPath}"
    }
  },
  "ResponseCacheSeconds": 60,
  "RespondsTo": "thumbs"
}
