{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Orchestrator"
    }
  },
  "Thumbs": {
    "ThumbsBucket": "thumbs-bucket-name"
  },
  "AWS": {
    "Profile": "default",
    "Region": "eu-west-1"
  },
  "DetailedErrors": true,
  "ConnectionStrings": {
    "PostgreSQLConnection": "***"
  },
  "ReverseProxy": {
    "Routes": {
      "catch_all": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "deliverator": {
        "Destinations": {
          "deliverator_orchestrator/one": {
            "Address": "http://localhost:5015",
            "Health": "http://localhost:5015/ping.aspx"
          }
        }
      }
    }
  }
}
