{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Orchestrator"
    }
  },
  "Thumbs": {
    "ThumbsBucket": "thumbs-bucket-name"
  },
  "Proxy": {
    "ThumbsPath": "thumbs",
    "CheckUVThumbs": true,
    "UVThumbReplacementPath": "!200,200",
    "S3HttpBase": "https://s3-eu-west-1.amazonaws.com",
    "StorageBucket": "storage-bucket-name",
    "ImageServerRoot": "http://localhost:5016",
    "CanResizeThumbs": true,
    "ThumbUpscaleConfig": {
      "CanUpsize": {
        "AssetIdRegex": "2/1/upsize.*",
        "UpscaleThreshold": 100
      }
    }
  },
  "AWS": {
    "Profile": "default",
    "Region": "eu-west-1"
  },
  "DetailedErrors": true,
  "ConnectionStrings": {
    "PostgreSQLConnection": "***"
  },
  "S3OriginRegex": "http\\:\\/\\/origin\\-bucket\\-name\\.s3\\.amazonaws\\.com\\/.*",
  "AuthServicesUriTemplate": "https://localhost:5003/auth/{customer}/{behaviour}",
  "ImageFolderTemplateImageServer": "/nas/{customer}/{space}/{image}.jp2",
  "ReverseProxy": {
    "Clusters": {
      "deliverator": {
        "Destinations": {
          "deliverator_orchestrator/one": {
            "Address": "http://localhost:5015"
          }
        }
      },
      "image_server": {
        "Destinations": {
          "image_server/one": {
            "Address": "http://localhost:5016"
          }
        }
      },
      "thumbs": {
        "Destinations": {
          "thumbs/one": {
            "Address": "http://localhost:5019"
          }
        }
      },
      "thumbresize": {
        "Destinations": {
          "thumbresize/one": {
            "Address": "http://localhost:5019"
          }
        }
      }
    }
  }
}
