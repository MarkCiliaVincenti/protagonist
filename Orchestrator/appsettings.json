{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Orchestrator"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "img_options": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-img/{**remainder}",
          "Methods": [
            "OPTIONS"
          ]
        }
      },
      "img_infojson": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-img/{cust}/{space}/{image}/info.json",
          "Methods": [
            "GET"
          ]
        }
      },
      "img_only": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-img/{cust}/{space}/{image}",
          "Methods": [
            "GET"
          ]
        }
      },
      "thumbkey": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/getThumbKey/{**remainder}",
          "Methods": [
            "GET"
          ]
        }
      },
      "thumbkey_exact": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/getThumbKeyExactSize/{**remainder}",
          "Methods": [
            "GET"
          ]
        }
      },
      "requiresAuth": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/requiresAuth/{**remainder}",
          "Methods": [
            "GET"
          ]
        }
      },
      "mapCustomerToID": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/mapCustomerToID/{**remainder}",
          "Methods": [
            "GET"
          ]
        }
      }
    }
  },
  "AllowedHosts": "*",
  "PathBase": ""
}